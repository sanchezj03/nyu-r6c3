<div class="inner">
	<%= remote_link(child_consents_url(@child), 'consent', :lose_focus => true) %>
	<h3>Update Consent</h3>

	<%= error_messages_for :consent %>
	<div class="indented">
		<% remote_update_form_for @consent, :element => 'consent', :url => child_consents_url(@child) do -%>
			<%= render :partial => 'form' %>
			<%= submit_tag "Save"%>
		<% end -%>
	</div>
</div>